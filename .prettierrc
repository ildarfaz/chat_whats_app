{
  "singleQuote": false,
  "trailingComma": "es5",
  "arrowParens": "always",
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "tabWidth": 2,
  "printWidth": 80,
  "importOrder": [
    "react",
    "<THIRD_PARTY_MODULES>",
    "^types/(.*)$",
    "^stores/(.*)$",
    "^(api|auth|app)/(.*)$",
    "^components/(.*)$",
    "^utils/(.*)$",
    "^styles/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}